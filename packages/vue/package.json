  "main": "lib",
  "module": "esm",
  "umd:main": "dist/formily.vue.umd.production.js",
  "unpkg": "dist/formily.vue.umd.production.js",
  "jsdelivr": "dist/formily.vue.umd.production.js",
  "jsnext:main": "esm",
  "types": "lib/index.d.ts",
  "engines": {
    "npm": ">=3.0.0"
  },
  "scripts": {
    "start": "vuepress dev docs",
