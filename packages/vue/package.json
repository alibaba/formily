    "@vuepress/plugin-back-to-top": "^1.8.2",
    "@vuepress/plugin-medium-zoom": "^1.8.2",
    "ant-design-vue": "^1.7.3",
    "core-js": "^2.4.0",
    "vuepress": "^1.8.2"
  },
  "dependencies": {
    "@formily/core": "^2.0.0-beta.3",
    "@formily/json-schema": "^2.0.0-beta.3",
    "@formily/reactive": "^2.0.0-beta.3",
